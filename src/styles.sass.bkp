.p-editor-container
  .p-editor-toolbar
    background: #1e1e1e
    border-top-right-radius: 3px
    border-top-left-radius: 3px

    &.ql-snow
      border: 1px solid #383838

      .ql-stroke
        stroke: rgba(255, 255, 255, 0.6)

      .ql-fill
        fill: rgba(255, 255, 255, 0.6)

      .ql-picker
        .ql-picker-label
          border: 0 none
          color: rgba(255, 255, 255, 0.6)

          &:hover
            color: rgba(255, 255, 255, 0.87)

            .ql-stroke
              stroke: rgba(255, 255, 255, 0.87)

            .ql-fill
              fill: rgba(255, 255, 255, 0.87)

        &.ql-expanded
          .ql-picker-label
            color: rgba(255, 255, 255, 0.87)

            .ql-stroke
              stroke: rgba(255, 255, 255, 0.87)

            .ql-fill
              fill: rgba(255, 255, 255, 0.87)

          .ql-picker-options
            background: #1e1e1e
            border: 1px solid #383838
            box-shadow: 0 2px 4px -1px rgba(0, 0, 0, 0.2), 0 4px 5px 0 rgba(0, 0, 0, 0.14), 0 1px 10px 0 rgba(0, 0, 0, 0.12)
            border-radius: 3px
            padding: 0.5rem 0

            .ql-picker-item
              color: rgba(255, 255, 255, 0.87)

              &:hover
                color: rgba(255, 255, 255, 0.87)
                background: rgba(255, 255, 255, 0.03)

          &:not(.ql-icon-picker) .ql-picker-item
            padding: 0.5rem 1rem

  .p-editor-content
    border-bottom-right-radius: 3px
    border-bottom-left-radius: 3px

    &.ql-snow
      border: 1px solid #383838

    .ql-editor
      background: #121212
      color: rgba(255, 255, 255, 0.87)
      border-bottom-right-radius: 3px
      border-bottom-left-radius: 3px

  .ql-snow.ql-toolbar
    button
      &:hover, &:focus
        color: rgba(255, 255, 255, 0.87)

      &:hover .ql-stroke, &:focus .ql-stroke
        stroke: rgba(255, 255, 255, 0.87)

      &:hover .ql-fill, &:focus .ql-fill
        fill: rgba(255, 255, 255, 0.87)

      &.ql-active
        color: #64B5F6

    .ql-picker-label.ql-active, .ql-picker-item.ql-selected
      color: #64B5F6

    button.ql-active .ql-stroke, .ql-picker-label.ql-active .ql-stroke, .ql-picker-item.ql-selected .ql-stroke
      stroke: #64B5F6

