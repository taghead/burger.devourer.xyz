    width: 20px
    height: 20px
    color: rgba(255, 255, 255, 0.87)
    border-radius: 3px
    transition: background-color 0.2s, color 0.2s, border-color 0.2s, box-shadow 0.2s

    .p-checkbox-icon
      transition-duration: 0.2s
      color: #212529
      font-size: 14px

    &.p-highlight
      border-color: #64B5F6
      background: #64B5F6

  &:not(.p-checkbox-disabled) .p-checkbox-box
