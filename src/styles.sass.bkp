      border-color: #64B5F6

    &.p-focus
      outline: 0 none
      outline-offset: 0
      box-shadow: 0 0 0 1px #93cbf9
      border-color: #64B5F6

    &.p-highlight:hover
      border-color: #2396f2
      background: #2396f2
      color: #212529

